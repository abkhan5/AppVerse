<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:AppVerse.Desktop.GameOfLife"
                    xmlns:views="clr-namespace:AppVerse.Desktop.GameOfLife.Views"
                    xmlns:viewModels="clr-namespace:AppVerse.Desktop.GameOfLife.ViewModels"    
                    xmlns:models="clr-namespace:AppVerse.Desktop.Models.GameOfLife;assembly=AppVerse.Desktop.Models"
                    
                    >



    <DataTemplate DataType="{x:Type viewModels:GameOfLifeLandingViewModel}">
        
        <views:GameOfLifeLandingView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModels:GameViewModel}">
        <views:GameView />
    </DataTemplate>



    <DataTemplate DataType="{x:Type viewModels:BoardViewModel}">

        <views:BoardView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:CellViewModel}">

        <views:CellView />
    </DataTemplate>

</ResourceDictionary>